{
  "collectCoverageFrom": [
    "<rootDir>/src/**/*.js",
    "!<rootDir>/src/index.js"
  ],
  "coverageReporters": [
    "lcov",
    "text"
  ],
  "preset": "jest-expo",
  "testMatch": [
    "<rootDir>/src/**/?(*.)+(test).js"
  ],
  "testPathIgnorePatterns": [
    "<rootDir>/node_modules/"
  ],
  "transform": {
    "^.+\\.js$": "babel-jest"
  }
}
