{
  "name": "expo-react-native-redux-starter",
  "description": "A React-Native starter application including Expo, Redux and more!",
  "main": "src/index.js",
  "scripts": {
    "eject": "expo eject",
    "eslint": "./node_modules/.bin/eslint src/ && echo \"\\x1b[102m\\x1b[1m ESLINT \\x1b[0m\" No JavaScript ESLint errors to fix",
    "eslint:fix": "./node_modules/.bin/eslint --fix src/ && echo \"\\x1b[102m\\x1b[1m ESLINT \\x1b[0m\" ESLint JavaScript errors fixed",
    "start": "expo start",
    "start:android": "expo start --android",
    "start:ios": "expo start --ios",
    "test": "./node_modules/.bin/jest --config ./jest.config.json",
    "test:coverage": "./node_modules/.bin/jest --config ./jest.config.json --coverage"
  },
  "author": {
    "name": "Joey Schroeder",
    "email": "schrjos@gmail.com"
  },
  "homepage": "https://github.com/joeyschroeder/expo-react-native-redux-starter",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/joeyschroeder/expo-react-native-redux-starter.git"
  },
  "dependencies": {
    "expo": "^31.0.6",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.1.tar.gz",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-preset-expo": "^5.0.0",
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.5.0",
    "jest-cli": "^23.6.0",
    "jest-expo": "^31.0.0",
    "prettier": "^1.15.3",
    "react-test-renderer": "^16.7.0"
  }
