{
  "name": "expo-react-native-redux-starter",
  "description": "A React-Native starter application including Expo, Redux and more!",
  "main": "src/index.js",
  "scripts": {
    "eject": "./node_modules/.bin/expo eject",
    "eslint": "./node_modules/.bin/eslint src/ && echo \"\\x1b[102m\\x1b[1m ESLINT \\x1b[0m\" No JavaScript ESLint errors to fix",
    "eslint:fix": "./node_modules/.bin/eslint --fix src/ && echo \"\\x1b[102m\\x1b[1m ESLINT \\x1b[0m\" ESLint JavaScript errors fixed",
    "start": "./node_modules/.bin/expo start --clear",
    "start:android": "npm run start -- --android",
    "start:ios": "npm run start -- --ios",
    "start:tunnel": "npm run start -- --tunnel",
    "start:web": "npm run start -- --web",
    "test": "./node_modules/.bin/jest --config ./jest.config.json",
    "test:coverage": "npm run test -- --coverage"
  },
  "author": {
    "name": "Joey Schroeder",
    "email": "schrjos@gmail.com"
  },
  "homepage": "https://github.com/joeyschroeder/expo-react-native-redux-starter",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/joeyschroeder/expo-react-native-redux-starter.git"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "expo": "^53.0.20",
    "expo-sensors": "~14.1.4",
    "expo-splash-screen": "~0.30.10",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-web": "^0.20.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/eslint-parser": "^7.28.0",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.33.0",
    "@expo/ngrok": "^4.1.3",
    "@testing-library/react-native": "13.2.2",
    "babel-preset-expo": "~13.2.3",
    "eslint": "^9.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-testing-library": "7.6.3",
    "expo-cli": "^6.3.12",
    "globals": "16.3.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-expo": "~53.0.9",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.0.0"
  },
  "private": true
}
