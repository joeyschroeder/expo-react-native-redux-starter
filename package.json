{
  "name": "expo-react-native-redux-starter",
  "description": "A React-Native starter application including Expo, Redux and more!",
  "main": "src/index.js",
  "scripts": {
    "eject": "./node_modules/.bin/expo eject",
    "eslint": "./node_modules/.bin/eslint src/ && echo \"\\x1b[102m\\x1b[1m ESLINT \\x1b[0m\" No JavaScript ESLint errors to fix",
    "eslint:fix": "./node_modules/.bin/eslint --fix src/ && echo \"\\x1b[102m\\x1b[1m ESLINT \\x1b[0m\" ESLint JavaScript errors fixed",
    "start": "./node_modules/.bin/expo start",
    "start:android": "./node_modules/.bin/expo start --android",
    "start:ios": "./node_modules/.bin/expo start --ios",
    "test": "./node_modules/.bin/jest --config ./jest.config.json",
    "test:coverage": "./node_modules/.bin/jest --config ./jest.config.json --coverage"
  },
  "author": {
    "name": "Joey Schroeder",
    "email": "schrjos@gmail.com"
  },
  "homepage": "https://github.com/joeyschroeder/expo-react-native-redux-starter",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/joeyschroeder/expo-react-native-redux-starter.git"
  },
  "dependencies": {
    "expo": "^32.0.6",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-redux": "^7.0.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-preset-expo": "^5.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-native": "^3.6.0",
    "jest-cli": "^24.7.1",
    "jest-dot-reporter": "^1.0.7",
    "jest-expo": "^32.0.0",
    "prettier": "^1.16.4",
    "react-test-renderer": "16.5.0"
  }
}
