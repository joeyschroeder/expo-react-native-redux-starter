{
  "name": "expo-react-native-redux-starter",
  "description": "A React-Native starter application including Expo, Redux and more!",
  "main": "src/index.js",
  "scripts": {
    "eject": "./node_modules/.bin/expo eject",
    "eslint": "./node_modules/.bin/eslint src/ && echo \"\\x1b[102m\\x1b[1m ESLINT \\x1b[0m\" No JavaScript ESLint errors to fix",
    "eslint:fix": "./node_modules/.bin/eslint --fix src/ && echo \"\\x1b[102m\\x1b[1m ESLINT \\x1b[0m\" ESLint JavaScript errors fixed",
    "start": "./node_modules/.bin/expo start --clear",
    "start:android": "npm run start -- --android",
    "start:ios": "npm run start -- --ios",
    "start:web": "npm run start -- --web",
    "test": "./node_modules/.bin/jest --config ./jest.config.json",
    "test:coverage": "npm run test -- --coverage"
  },
  "author": {
    "name": "Joey Schroeder",
    "email": "schrjos@gmail.com"
  },
  "homepage": "https://github.com/joeyschroeder/expo-react-native-redux-starter",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/joeyschroeder/expo-react-native-redux-starter.git"
  },
  "dependencies": {
    "expo": "^44.0.6",
    "expo-app-loading": "~1.3.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-web": "0.17.1",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "babel-eslint": "^10.1.0",
    "babel-preset-expo": "9.0.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-native": "^3.11.0",
    "expo-cli": "^4.13.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-dot-reporter": "^1.0.14",
    "jest-expo": "^44.0.1",
    "prettier": "^2.5.1",
    "react-test-renderer": "^17.0.2"
  },
  "private": true
}
